export PS1="\u[\w] > "
export CLICOLOR=1
alias ls='ls -G'

export LC_CTYPE=en_US.UTF-8
export LC_ALL=en_US.UTF-8

alias code='code-insiders'

export REACT_EDITOR=code

#export RCT_METRO_PORT=8082


export NVM_DIR="$HOME/.nvm"
